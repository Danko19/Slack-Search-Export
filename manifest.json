{
	"name": "Slack Search Export",
	"action": {},
	"description": "Export your Slack search result",
	"version": "1.0",
	"manifest_version": 3,
	"permissions": [
		"webRequest"
	],
	"host_permissions": [
		"https://*.slack.com/*"
	],
	"content_scripts": [
		{
			"matches": [
				"https://*.slack.com/*"
			],
			"js": [
				"content-script.js"
			]
		}
	],
	"background": {
		"service_worker": "background.js"
	}
}